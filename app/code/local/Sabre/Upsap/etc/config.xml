<config>

    <modules>
        <Sabre_Upsap>
            <version>0.0.2</version>
        </Sabre_Upsap>
    </modules>

    <global>
        <resources>
            <sabre_upsap_setup>
                <setup>
                    <module>Sabre_Upsap</module>
                </setup>
            </sabre_upsap_setup>
        </resources>
    </global>

    <frontend>
        <routers>
            <upsap>
                <args>
                    <modules>
                        <sabre_upsap before="Infomodus_Upsap">Sabre_Upsap</sabre_upsap>
                    </modules>
                </args>
            </upsap>
        </routers>
    </frontend>

</config>